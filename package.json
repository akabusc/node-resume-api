{
  "name": "node-resume-function",
  "version": "1.0.0",
  "description": "NodeJS Resume function",
  "main": "app.js",
  "author": "Jon Carson",
  "license": "MIT",
  "dependencies": {
    "@types/aws-lambda": "^8.10.50",
    "@types/node": "^13.13.0",
    "axios": "^0.18.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "test": "mocha tests/unit/",
    "postinstall": "rm -rf dist && tsc",
    "lint": "eslint --no-error-on-unmatched-pattern ./src/**/*.ts",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix ./src/**/*.ts",
    "prettier:base": "prettier './src/**/*.ts'",
    "prettier:check": "npm run prettier:base -- --check",
    "prettier:write": "npm run prettier:base -- --write"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^6.1.4",
    "lint-staged": "^10.1.5",
    "prettier": "^2.0.4"
  },
  "files": [
    "src/**/*",
    "app.js",
    "tsconfig.json"
  ],
  "lint-staged": {
    "src/**/*": [
      "npm run lint:fix",
      "npm run prettier:write"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true
  }
}
